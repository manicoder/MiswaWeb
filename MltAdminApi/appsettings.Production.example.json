{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Host=your-production-db-host;Database=your-production-db;Username=your-db-user;Password=your-db-password;"
  },
  "Jwt": {
    "Key": "your-production-jwt-secret-key-here",
    "Issuer": "MLT-Admin-API",
    "Audience": "MLT-Admin-Client"
  },
  "Encryption": {
    "Key": "your-production-encryption-key-here"
  },
  "MailerSend": {
    "SmtpUsername": "your_actual_mailersend_smtp_username",
    "SmtpPassword": "your_actual_mailersend_smtp_password",
    "FromEmail": "noreply@yourdomain.com",
    "FromName": "MLT Admin"
  }
} 