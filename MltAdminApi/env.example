# MailerSend Configuration
# Replace these placeholder values with your actual MailerSend credentials

# Your MailerSend SMTP username (from MailerSend dashboard)
MAILERSEND_SMTP_USERNAME=your_smtp_username_here

# Your MailerSend SMTP password (from MailerSend dashboard)  
MAILERSEND_SMTP_PASSWORD=your_smtp_password_here

# The email address that will appear as the sender
# Must be from your verified domain in MailerSend
MAILERSEND_FROM_EMAIL=noreply@yourdomain.com

# The name that will appear as the sender
MAILERSEND_FROM_NAME=MLT Admin

# Optional: Database connection (if not using appsettings.json)
# ConnectionStrings__DefaultConnection=Host=your-db-host;Database=your-db;Username=your-user;Password=your-password;

# Optional: JWT settings (if not using appsettings.json)
# Jwt__Key=your-jwt-secret-key-here
# Jwt__Issuer=MLT-Admin-API
# Jwt__Audience=MLT-Admin-Client 